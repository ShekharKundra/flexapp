<% include ../partial/header %>
<br>

<div class="container" style="background-color:floralwhite; padding: 20px">

    <h2>
        <b>Double Arch</b> Rubber Expansion Joint
    </h2>
    <form method="POST" action="">
        <div class="container">
            <div class="row" style="background-color:#EAF2F8; font-size:20px">
                <div class="col-sm-2" value="" align=center>
                    <span>Size
                        <select class="form-control" name="size" id="size">
                            <option value=""></option>
                            <option value="25">25</option>
                            <option value="32">32</option>
                            <option value="40">40</option>
                            <option value="50">50</option>
                            <option value="65">65</option>
                            <option value="80">80</option>
                            <option value="100">100</option>
                            <option value="125">125</option>
                            <option value="150">150</option>
                            <option value="200">200</option>
                            <option value="250">250</option>
                            <option value="300">300</option>
                            <option value="350">350</option>
                            <option value="400">400</option>
                            <option value="450">450</option>
                            <option value="500">500</option>
                            <option value="550">550</option>
                            <option value="600">600</option>
                            <option value="700">700</option>
                            <option value="750">750</option>
                        </select></span>
                </div>
                <div class="col-sm-2" value="" align=center>
                    <span>Drilling Standard
                        <select class="form-control" name="drilling" id="drilling">
                            <option value=""></option>
                            <option value="None">None</option>
                            <option value="Table D">Table D</option>
                            <option value="ANSI">ANSI</option>
                            <option value="Table E">Table E</option>
                            <option value="BS 4504">BS 4504</option>
                            <option value="OT">Other</option>
                        </select> </span>
                </div>
                <div class="col-sm-3" value="" align=center>
                    <span>Quantity<br>
                        <button type="submit" class="btn btn-primary">Find</button>
                    </span>
                </div>
                <div class="col-sm-5" value="" align=center>
                    <span></span>
                </div>
            </div>
        </div>
    </form>
    <div class="row" style="background-color:orange; font-size:22px; padding:10px;">
        <div class="col-sm-2" value="" align=center>
            <span>Size
            </span>
        </div>
        <div class="col-sm-2" value="" align=center>
            <span>Drilling

            </span>
        </div>
        <div class="col-sm-3" value="" align=center>
            <span>Required Stock
            </span>
        </div>
        <div class="col-sm-2" value="" align=center>
            <span>Current Stock
            </span>
        </div>
        <div class="col-sm-3" value="" align=center>
            <span>Additional Stock Required
            </span>
        </div>
    </div>
    <% data.forEach(function(dt, index){ %>
    <div class="row" style="background-color:#FDFEFE;border-bottom: 1px solid orange; font-size:18px;padding:8px;">
        <div class="col-sm-3" align=center>
            <span><br>
                <%= dt.ordersize %>
            </span>
        </div>
        <div class="col-sm-2" align=center>
            <span><br>
                <%= dt.orderdrilling %>
            </span>
        </div>
        <div class="col-sm-3" align=center>
            <span><br>

            </span>
        </div>
        <div class="col-sm-2" align=center>
            <span><br>

            </span>
        </div>
        <div class="col-sm-2" align=center>
            <span>
            </span>

        </div>
    </div>
    <% }) %>
</div>

<% include ../partial/footer %>